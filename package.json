  "scripts": {
    "babel:compile": "babel src --out-dir lib --ignore vendor",
    "babel:watch": "babel src --watch --out-dir lib --ignore vendor",
    "build": "npm run lint && npm run clean && npm run copy && npm run babel:compile",
    "clean": "rimraf lib/*",
    "copy": "cp -r src/ lib/",
    "demo": "npm run server & gulp & wait",
    "dev": "npm run clean && npm run copy && npm run babel:watch",
    "eslint": "./node_modules/eslint/bin/eslint.js",
    "lint": "eslint src",
    "server": "http-server ./demo/dist",
    "prepublish": "npm run build",
    "test": "echo \"Warning: no test specified\" && exit 0"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "tiny-emitter": "^1.1.0",
    "zero-fill": "2.2.3"
  },
